	, setUI : function(which, id, value) {
		if ( which in this.uiStates && id in this.uiStates[which] ) {
			this.uiStates[which][id] = value;
			window.localStorage.setItem('crmUI', JSON.stringify(this.uiStates));

			return true;
		}

		return false;
	}
