					var ai_token_have_count = $("#ai_token_have_count").val(); // 1 digit decimal
					var ai_token_need_count = $("#ai_token_need_count").val(); // Whole Number

					ai_token_amount = ai_token_have_count - (ai_token_rate * ai_token_need_count);

